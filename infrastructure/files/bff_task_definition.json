[
  {
    "image": "${image}:latest",
    "name": "${container_name}",
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": ${app_port},
        "hostPort": ${app_port}
      }
    ],
    "mountPoints": [],
    "volumesFrom": [],
    "systemControls": [],
    "essential": true,
    "environment": [
      {
        "name": "PLACES_URL",
        "value": "${places_url}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${aws_log_group}",
        "awslogs-region": "${aws_region}",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]